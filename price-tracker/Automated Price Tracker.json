    {
      "parameters": {
        "jsCode": "const item = $input.item.json;\n\nlet rawPrice = item.price_preferred || item.price_fallback || item.fiyat_raw;\n\nif (!rawPrice) {\n  return { json: { error: \"Price not found. Check selectors.\", ...item } };\n}\n\n\nif (typeof rawPrice === 'object') {\n    rawPrice = JSON.stringify(rawPrice);\n}\n\n\n\nlet cleanPrice = rawPrice.toString().replace(/\\r?\\n|\\r/g, '').replace(/\\s+/g, '');\n\nlet currency = \"TL\"; // Default fallback\nif (cleanPrice.includes('$') || cleanPrice.includes('USD')) currency = \"$\";\nelse if (cleanPrice.includes('‚Ç¨') || cleanPrice.includes('EUR')) currency = \"‚Ç¨\";\nelse if (cleanPrice.includes('¬£') || cleanPrice.includes('GBP')) currency = \"¬£\";\n\nif (cleanPrice.includes('$') || cleanPrice.includes('‚Ç¨') || cleanPrice.includes('USD')) {\n    cleanPrice = cleanPrice.replace(/[^\\d.]/g, '');\n} else {\n    cleanPrice = cleanPrice\n        .replace(/\\./g, '')      \n        .replace(',', '.')       \n        .replace(/[^\\d.]/g, ''); \n}\n\nconst currentPrice = parseFloat(cleanPrice);\nconst targetPrice = parseFloat( $('Get row(s) in sheet').item.json.TargetPrice );\nconst lastPrice = parseFloat( $('Get row(s) in sheet').item.json.LastPrice );\n\nconst isDiscount = currentPrice < lastPrice && currentPrice > 0;\nconst isPriceIncrease = currentPrice > lastPrice && lastPrice > 0;  \nconst isTargetHit = currentPrice <= targetPrice && currentPrice > 0;\n\nlet alertReason = \"\";\nif (isTargetHit) {\n    alertReason = \"Target Price Hit!\";\n} else if (isDiscount) {\n    alertReason = \"Discount Alert!\";\n} else if (isPriceIncrease) {\n    alertReason = \"Price Increased!\";\n}\n\n\nconst isAlert = isDiscount || isTargetHit || isPriceIncrease;\n\nreturn {\n  json: {\n    ...item,\n    currentPrice: currentPrice,\n    currency: currency,\n    isAlert: isAlert,\n    alertReason: alertReason || \"No Change\"\n  }\n};\n"
      },
